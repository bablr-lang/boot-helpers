{
  "name": "@bablr/boot-helpers",
  "version": "0.0.0",
  "description": "Runtime helpers for bootstrapping BABLR VM",
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "exports": {
    "./decorators": "./lib/decorators.js",
    "./path": "./lib/path.js",
    "./productions": "./lib/productions.generated.js",
    "./symbols": "./lib/symbols.js",
    "./types": "./lib/types.js"
  },
  "files": [
    "lib/**/*.js"
  ],
  "sideEffects": false,
  "scripts": {
    "generate": "babel lib/productions.js --out-file lib/productions.generated.js",
    "clean": "rm lib/productions.generated.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.22.15",
    "@babel/core": "7.22.20",
    "@babel/plugin-proposal-decorators": "7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/runtime": "^7.23.2",
    "@bablr/boot": "github:bablr-lang/boot#311977defee968aa8454f9de9c7355fb93df0fc9",
    "@bablr/eslint-config-base": "github:bablr-lang/eslint-config-base#a49dbe6861a82d96864ee89fbf0ec1844a8a5e8e",
    "babel-plugin-macros": "3.1.0",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^8.47.0",
    "eslint-import-resolver-enhanced-resolve": "^1.0.5",
    "eslint-plugin-import": "^2.27.5",
    "expect": "^29.6.2",
    "prettier": "^2.0.5"
  },
  "repository": "git@github.com:bablr-lang/boot-helpers.git",
  "homepage": "https://github.com/bablr-lang/boot-helpers",
  "author": "Conrad Buck <conartist6@gmail.com>",
  "license": "MIT"
}
